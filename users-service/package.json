    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@forestfire/core": "1.0.0",
    "@hapi/boom": "^9.0.0",
    "@hapi/hapi": "^18.4.0",
    "bcrypt": "^3.0.7",
    "dotenv": "^8.2.0",
    "hapi-auth-jwt2": "^8.8.1",
    "pg": "^7.17.0",
